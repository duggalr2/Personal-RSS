    var text = '';
    document.addEventListener('click', function(e) {
            e = e || window.event;
            var target = e.target || e.srcElement;
            text = target.textContent || text.innerText;
            console.log(text);
            updatePieFact(text)
        }, false);


    function updatePieFact(p){
        var URL = "/home/";
        var data = {'pieFact': p};
            $.post(URL, data, function(response){
                if(response === 'success'){
                    console.log('nice');
                }
                else{ console.log('Error! :('); }
            });
        }




























































                                    
                                    

                                    
                                        
                                            
